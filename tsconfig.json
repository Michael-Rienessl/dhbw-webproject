{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2020",
        "removeComments": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "newLine": "lf",
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "typeRoots": ["@types"],
        "esModuleInterop": true,
        "outDir": "./js/"
    }
}