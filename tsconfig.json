{
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ES2020",
      "allowJs": false,
      "removeComments": true,
      "noEmitOnError": false,
      "noFallthroughCasesInSwitch": false,
      "noUnusedLocals": true,
      "newLine": "lf",
      "noImplicitAny": false,
      "noUnusedParameters": true,
      "typeRoots": ["@types"],
      "esModuleInterop": true,
      "outDir": "./js/"
    },
  "exclude": [
    "./src/dev_placeholder.ts",
    "node_modules"
  ]
}