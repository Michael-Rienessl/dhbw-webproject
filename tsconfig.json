{
    "compilerOptions": {
      "module": "CommonJS",
      "target": "ES2020",
      "removeComments": true,
      "noEmitOnError": false,
      "noFallthroughCasesInSwitch": false,
      "noUnusedLocals": true,
      "newLine": "lf",
      "noImplicitAny": false,
      "noUnusedParameters": true,
      "typeRoots": ["@types"],
      "esModuleInterop": true,
      "outDir": "/dist/"
    }
  }